{
  "name": "novablox",
  "version": "1.1.0",
  "description": "Roblox Studio bridge server and integrations for autonomous AI control",
  "private": true,
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "check": "node --check server/index.js && node --check server/command_store.js && node --check server/rate_limiter.js && node --check server/command_catalog.js && node --check server/assistant_engine.js && node --check extensions/openclaw/roblox-bridge/index.js && node --check examples/mock/mock_studio_client.js && node --check scripts/run_showcase.js && node --check scripts/run_ultimate_demo.js && node --check scripts/sync_studio_settings.js && node --check scripts/doctor.js",
    "lint": "npm run check && npm test",
    "format": "prettier --write \"README.md\" \"INSTALL.md\" \"QUICK_START.md\" \"BuyerGuide.md\" \"CHANGELOG.md\" \"docs/**/*.md\" \"server/**/*.js\" \"scripts/**/*.js\" \"tests/**/*.js\" \"extensions/**/*.js\" \"examples/**/*.js\" \"package.json\"",
    "format:check": "prettier --check \"README.md\" \"INSTALL.md\" \"QUICK_START.md\" \"BuyerGuide.md\" \"CHANGELOG.md\" \"docs/**/*.md\" \"server/**/*.js\" \"scripts/**/*.js\" \"tests/**/*.js\" \"extensions/**/*.js\" \"examples/**/*.js\" \"package.json\"",
    "test": "node --test tests/*.test.js",
    "bootstrap:macos-arm64": "bash scripts/bootstrap-macos-arm64.sh",
    "secure:local": "bash scripts/secure_local_env.sh",
    "studio:sync": "node scripts/sync_studio_settings.js",
    "doctor": "node scripts/doctor.js",
    "showcase:run": "node scripts/run_showcase.js",
    "showcase:ultimate": "node scripts/run_ultimate_demo.js",
    "mock:studio": "node examples/mock/mock_studio_client.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.34.41",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "prettier": "^3.6.2"
  }
}
